&spi1 {
	compatible = "nordic,nrf-spi";
	status = "okay";
	
	sck-pin = <26>;
	mosi-pin = <28>;
	miso-pin = <31>;
	cs-gpios = <&gpio0 30 GPIO_ACTIVE_LOW>;
	
};

&spi0 {
	compatible = "nordic,nrf-spi";
	status = "okay";
	
	sck-pin = <2>;
	mosi-pin = <6>;
	miso-pin = <4>;
	cs-gpios = <&gpio0 8 GPIO_ACTIVE_LOW>;
	
};